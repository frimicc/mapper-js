{
  "name": "mapper-js",
  "version": "1.0.0",
  "description": "React webapp for hex-based map making.",
  "main": "index.js",
  "scripts": {
    "start": "babel-node src/server",
    "test": "yarn lint && flow && jest --coverage",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "cross-env NODE_ENV=production babel-node src/server",
    "prod:stop": "pm2 delete server",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frimicc/mapper-js.git"
  },
  "author": "Michael (frimicc)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frimicc/mapper-js/issues"
  },
  "homepage": "https://github.com/frimicc/mapper-js#readme",
  "browserslist": [
    "> 1%"
  ],
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "react": "^16.1.0",
    "react-dom": "^16.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
